{
  "build": {
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build",
    "devPath": "http://localhost:1420",
    "distDir": "../build"
  },
  "package": {
    "productName": "Synapse X",
    "version": "1.1.1"
  },
  "tauri": {
    "allowlist": {
      "all": false,
      "shell": {
        "all": false,
        "open": true
      },
      "dialog": {
        "open": true,
        "save": true
      },
      "fs": {
        "readFile": true,
        "writeFile": true,
        "readDir": true,
        "scope": ["$APPDATA/scripts", "$APPDATA/scripts/*"]
      },
      "window": {
        "all": false,
        "close": true,
        "hide": true,
        "show": true,
        "maximize": true,
        "minimize": true,
        "unmaximize": true,
        "unminimize": true,
        "startDragging": true,
        "setAlwaysOnTop": true
      }
    },
    "windows": [
      {
        "title": "Synapse X - v1.1.1",
        "label": "main",
        "width": 801,
        "height": 355,
        "decorations": false,
        "alwaysOnTop": false
      }
    ],
    "security": {
      "csp": null
    },
    "bundle": {
      "active": true,
      "targets": "all",
      "identifier": "fun.raptor.synapsploit",
      "icon": [
        "icons/16x16.png",
        "icons/24x24.png",
        "icons/32x32.png",
        "icons/48x48.png",
        "icons/256x256.png",
        "icons/ico.icns",
        "icons/icon.ico"
      ]
    },
    "systemTray": {
      "iconPath": "icons/256x256.png",
      "iconAsTemplate": true
    }
  }
}
